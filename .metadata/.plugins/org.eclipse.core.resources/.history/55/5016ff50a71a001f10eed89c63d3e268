package manager;

import javax.ejb.Stateless;
import javax.persistence.EntityManager;
import javax.persistence.PersistenceContext;

@Stateless
public class TestManager {
	
	@PersistenceContext
	EntityManager em;
	
	public void saveAlain() {
		Admin a = new Admin();
		a.setName("Alain");
		em.persist(a);
	}
	

}
