package beans;

import java.io.Serializable;
import java.time.LocalDate;

import javax.persistence.Entity;

@Entity
public class Admin extends Person implements Serializable {
    
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public Admin() {}
	
	public Admin(String ldap, String password, String name, String lastname, String email, String phone, LocalDate birthdate) {
		super(ldap, password, name, lastname, email, phone, birthdate);
		System.out.println("creation of admin : " + name + ", " + lastname);
	}
	
}
